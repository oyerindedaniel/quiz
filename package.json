{
  "name": "quiz-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "node scripts/seed.js",
    "electron:dev": "pnpm run seed && cross-env NODE_ENV=development pnpm run electron:compile && concurrently \"cross-env NODE_ENV=development pnpm run dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development pnpm exec electron dist/electron/main.js\"",
    "electron:build": "pnpm run build && pnpm run electron:compile",
    "electron:compile": "tsc --project tsconfig.electron.json",
    "electron:pack": "pnpm run electron:build && electron-builder",
    "electron:dist": "pnpm run electron:build && electron-builder --publish=never"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.1",
    "electron-rebuild": "^3.2.9",
    "next": "15.3.3",
    "pg": "^8.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "tsx": "^4.19.4",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  },
  "packageManager": "pnpm@9.13.2+sha512.88c9c3864450350e65a33587ab801acf946d7c814ed1134da4a924f6df5a2120fd36b46aab68f7cd1d413149112d53c7db3a4136624cfd00ff1846a0c6cef48a"
}
